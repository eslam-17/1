import random

suits = ["Clubs", "Spades", "Hearts","Diamonds" ]
faces = ["Jack", "Queen", "King", "Ace"]
numbered= [2,3,4,5,6,7,8,9,10]

# Create the deck of cards
deck = set()
for suit in suits:
    for card in faces + numbered :
        deck.add(f"{card} of {suit}") 

# Shuffle the deck
deck = list(deck)
random.shuffle(deck)

# Deal the cards to the four players
player1 = []
player2 = []
player3 = []
player4 = []

for i, card in enumerate(deck):
    if i % 4 == 0:
        player1.append(card)
    elif i % 4 == 1:
        player2.append(card)
    elif i % 4 == 2:
        player3.append(card)
    else:
        player4.append(card)

# Print the cards for each player
print("Player 1's cards:")
for card in player1:
    print(card)

print("\nPlayer 2's cards:")
for card in player2:
    print(card)

print("\nPlayer 3's cards:")
for card in player3:
    print(card)

print("\nPlayer 4's cards:")
for card in player4:
    print(card)
